{
    "name": "Privacy Plugin",
    "version": "1.0.0",
    "description": "just shows site on current tab",
    "manifest_version": 3,
    "author": "Gavin Drakeford",
    "action":{
        "default_popup": "index.html",
        "default_title": "Current URL"
    },
    "permissions": [
        "tabs",
        "storage"
    ],
    "icons":{
        "512":"extension_icon.png"
    }
}